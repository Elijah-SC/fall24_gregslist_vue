{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ],
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "vue.codeActions.enabled": true,
  "vue.codeLens.enabled": true,
  "vue.splitEditors.icon": true,
  "eslint.useFlatConfig": true,
  "eslint.enable": true,
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.debug": true,
  "eslint.format.enable": true,
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#27344d",
    "activityBar.background": "#27344d",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#944b64",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#27344d",
    "statusBar.background": "#161d2b",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#27344d",
    "statusBarItem.remoteBackground": "#161d2b",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#161d2b",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#161d2b99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#161d2b",
}